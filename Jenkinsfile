pipeline {
	agent any
	stages {
		stage('checkout') {
			steps {
		   		echo 'you can check out the code from git hub'
				}
		}
		stage('build') {
			steps {
				echo 'can perform a build using any build tool or shell script'
				}
		}
		stage('test') {
			steps {   
				echo 'can perform testing using testing tool like junit'
				}
		}
		stage('deploy') {
			steps {   
				echo 'can deply the code in containers or deployment VMs'
				}
		}
	}
}
